{
  "name": "tiltakspenger-meldekort",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "npm run dev --workspace server",
    "start": "npm run start --workspace server",
    "build": "npm-run-all build:*",
    "build:client": "vite build --outDir server/dist/client",
    "build:ssr": "vite build --ssr src/main-server.tsx --outDir server/src/_ssr-dist",
    "build:server": "npm run build --workspace server",
    "lint": "npm-run-all --parallel lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:tsc-server": "npm run tsc --workspace server",
    "lint:eslint": "eslint \"**/*.ts\" \"**/*.tsx\"",
    "test": "playwright test"
  },
  "workspaces": [
    "server"
  ],
  "dependencies": {
    "@navikt/aksel-icons": "7.10.0",
    "@navikt/ds-css": "7.10.0",
    "@navikt/ds-react": "7.10.0",
    "@navikt/nav-dekoratoren-moduler": "3.1.3",
    "@preact/compat": "18.3.1",
    "dayjs": "1.11.13",
    "preact-render-to-string": "6.5.13",
    "swr": "2.3.0",
    "wouter": "3.5.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.10.1",
    "@playwright/test": "^1.50.1",
    "@preact/preset-vite": "2.10.1",
    "@types/node": "22.13.1",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@typescript-eslint/eslint-plugin": "8.24.0",
    "@typescript-eslint/parser": "8.39.0",
    "@vitejs/plugin-react": "4.7.0",
    "eslint": "8.57.1",
    "eslint-plugin-css-modules": "2.12.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rollup-plugin-visualizer": "5.14.0",
    "typescript": "5.7.3",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ],
    "*.{ts,tsx,css}": [
      "prettier --write"
    ]
  }
}
